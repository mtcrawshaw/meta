{
    "trainer": "ContinualTrainer",
    "dataset": "Rotated",
    "num_updates": 250,
    "batch_size": 1024,
    "num_workers": 8,

    "dataset_kwargs": {
        "download": true,
        "dataset": "CIFAR10",
        "num_tasks": 10,
        "alpha": 18.0
    },

    "lr_schedule_type": null,
    "initial_lr": 7e-4,
    "final_lr": 7e-4,
    "max_grad_norm": 0.5,
    "eps": 1e-5,

    "architecture_config": {
        "type": "conv",
        "num_conv_layers": 5,
        "initial_channels": 32,
        "num_fc_layers": 2,
        "fc_hidden_size": 64,
        "activation": "relu",
        "batch_norm": true
    },

    "evaluation_freq": 50,
    "continual_bn": "separate",

    "cuda": true,
    "seed": 1,
    "print_freq": 1,
    "save_freq": null,
    "load_from": null,
    "metrics_filename": null,
    "baseline_metrics_filename": null,
    "save_name": null
}
