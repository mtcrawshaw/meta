{
    "env_name": "CartPole-v1",

    "num_updates": 100,
    "rollout_length": 32,
    "num_ppo_epochs": 1,
    "num_minibatch": 1,
    "num_processes": 4,

    "lr_schedule_type": "exponential",
    "initial_lr": 7e-4,
    "final_lr": 7e-4,
    "eps": 1e-5,
    "value_loss_coeff": 0.5,
    "entropy_loss_coeff": 0.01,
    "gamma": 0.99,
    "gae_lambda": 0.95,
    "max_grad_norm": 0.5,
    "clip_param": 0.2,
    "clip_value_loss": true,
    "normalize_advantages": true,
    "normalize_transition": true,

    "num_layers": 3,
    "hidden_size": 64,
    "recurrent": true,

    "evaluation_freq": 10,
    "evaluation_episodes": 10,

    "cuda": true,
    "seed": 1,
    "print_freq": 10,
    "time_limit": null,
    "metrics_filename": null,
    "baseline_metrics_filename": null,
    "save_name": "save_test"
}
