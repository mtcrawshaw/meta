{
    "trials_per_method": 10,

    "base_train_config": {
        "trainer": "SLTrainer",
        "dataset": "NYUv2",
        "num_updates": 660,
        "batch_size": 36,
        "num_workers": 8,

        "dataset_kwargs": {
            "task_type": "multitask",
            "scale": 0.25
        },
        "loss_weighter": {
            "type": "Constant",
            "loss_weights": null
        },

        "lr_schedule_type": null,
        "initial_lr": 7e-4,
        "final_lr": 7e-4,
        "max_grad_norm": 0.5,
        "eps": 1e-5,

        "architecture_config": {
            "type": "backbone",
            "arch_type": "resnet",
            "num_backbone_layers": 50,
            "num_head_layers": 3,
            "head_channels": 256,
            "pretrained": true
        },

        "evaluation_freq": 33,

        "cuda": true,
        "print_freq": 1,
        "save_freq": null,
        "load_from": null,
        "metrics_filename": null,
        "baseline_metrics_filename": null
    },

    "methods": {
        "Constant": {"loss_weighter": {"type": "Constant"}},
        "Uncertainty": {"loss_weighter": {"type": "Uncertainty"}},
        "GradNorm": {"loss_weighter": {"type": "GradNorm", "asymmetry": 1.5, "weight_lr": 0.025}},
        "DWA": {"loss_weighter": {"type": "DWA", "ema_alpha": 0.9, "temp": 2.0}},
        "PCGrad": {"PCGrad": "true"},
        "SLAW": {"loss_weighter": {"type": "SLAW"}}
    },

    "seed": 0,
    "save_name": "NYUv2"
}
